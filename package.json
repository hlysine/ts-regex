{
  "name": "ts-regex",
  "version": "1.0.0",
  "author": "Henry Lin",
  "license": "MIT",
  "description": "Strongly-typed regex checker written entirely in TypeScript type definitions.",
  "keywords": [
    "regex",
    "regexp",
    "readable",
    "verbal",
    "regular",
    "expression",
    "typescript",
    "typed"
  ],
  "homepage": "https://github.com/hlysine/ts-regex#readme",
  "repository": "github:hlysine/ts-regex",
  "bugs": {
    "url": "https://github.com/hlysine/ts-regex/issues"
  },
  "main": "dist/ts-regex.umd.js",
  "module": "dist/ts-regex.mjs",
  "typings": "dist/ts-regex.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup --config rollup.config.ts --configPlugin rollup-plugin-esbuild",
    "watch": "rollup --watch --config rollup.config.ts --configPlugin rollup-plugin-esbuild",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "test": "jest"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@types/jest": "^29.5.1",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "esbuild": "^0.17.19",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "rollup": "^3.22.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
